package com.yonyou.scf.common.bank.lccb;

import java.io.Serializable;

public class F639Request implements Serializable{

	private static final long serialVersionUID = -5915113422196742829L;
	//商户号
	private String busiNum;
	//借据号
	private String fcsSerialNum;
	//还款种类
	private String repayType;
	//部分还款类型
	private String repayMode;
	//金额
	private Double amount;
	//币种
	private Double currency;
	//备用字段
	private String filed1;
	//备用字段
	private Double filed2;
	//备用字段
	private Double filed3;
	public String getBusiNum() {
		return busiNum;
	}
	public void setBusiNum(String busiNum) {
		this.busiNum = busiNum;
	}
	public String getFcsSerialNum() {
		return fcsSerialNum;
	}
	public void setFcsSerialNum(String fcsSerialNum) {
		this.fcsSerialNum = fcsSerialNum;
	}
	public String getRepayType() {
		return repayType;
	}
	public void setRepayType(String repayType) {
		this.repayType = repayType;
	}
	public String getRepayMode() {
		return repayMode;
	}
	public void setRepayMode(String repayMode) {
		this.repayMode = repayMode;
	}
	public Double getAmount() {
		return amount;
	}
	public void setAmount(Double amount) {
		this.amount = amount;
	}
	public Double getCurrency() {
		return currency;
	}
	public void setCurrency(Double currency) {
		this.currency = currency;
	}
	public String getFiled1() {
		return filed1;
	}
	public void setFiled1(String filed1) {
		this.filed1 = filed1;
	}
	public Double getFiled2() {
		return filed2;
	}
	public void setFiled2(Double filed2) {
		this.filed2 = filed2;
	}
	public Double getFiled3() {
		return filed3;
	}
	public void setFiled3(Double filed3) {
		this.filed3 = filed3;
	}
	
	
}
