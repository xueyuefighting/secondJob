        $("#success").hide();
        $("#finish").hide();

        $("#UShield").show();
        textReturnUrl();
    });
	
	function textReturnUrl(){
	 var ReturnURL = getQueryString("ReturnURL");
		if(!ReturnURL || ReturnURL==""){
    		$(".fanhui").remove();
    	}else{
    		$(".fanhui").attr("onclick","transfor('"+ReturnURL+"');");
    	}
	}
	
	function transfor(ReturnURL){
		 window.location.href=ReturnURL;
	}
	
    $(".sendSuccess").click(function () {
        FundID = getQueryString("FundID");
        MerchantUserID = getQueryString("MerchantUserID");
        UserRole = getQueryString("UserRole");
        ReturnURL = getQueryString("ReturnURL");
        PlatformId = getQueryString("PlatformId");
        InterfaceNo = getQueryString("InterfaceNo");
        TerminalSource = getQueryString("TerminalSource");
        //构造ajax请求参数
        var obj = {};
        obj.FundID = FundID;
        obj.MerchantUserID = MerchantUserID;
        obj.UserRole = UserRole;
        obj.ReturnURL = ReturnURL;
        obj.PlatformId = PlatformId;
        obj.InterfaceNo = InterfaceNo;
        obj.TerminalSource = TerminalSource;
        var requestData = JSON.stringify(obj);
		var urlParam = "?";
		urlParam+="FundID="+FundID+"&MerchantUserID="+MerchantUserID;
		urlParam+="&UserRole="+UserRole+"&ReturnURL="+ReturnURL;
		urlParam+="&PlatformId="+PlatformId+"&InterfaceNo="+InterfaceNo;
		urlParam+="&TerminalSource="+TerminalSource;
		
		/* $.getJSON($server_IP+urlParam,function(data){   
		                 $("#contract").hide();
                $("#finish").show();
                $(".yanzhen").find("img").attr("src","../../images/buzhou3.png");
                textReturnUrl();
		 
        //通过循环取出data里面的值         
    });  */
		
       $.ajax({
           type:"POST",
            url:$server_IP,
            dataType:"JSON",
            contentType:"application/json",
            data:requestData,
            success:function(data){
                $("#contract").hide();
                $("#finish").show();
                $(".yanzhen").find("img").attr("src","../../images/buzhou3.png");
                textReturnUrl();
            },error:function(){
                alert("系统有误");
            }
        });
    });

    $(".sendFail").click(function () {
        //
    });

    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
    </script>
    
   
