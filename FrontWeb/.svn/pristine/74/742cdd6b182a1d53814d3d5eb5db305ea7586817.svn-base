.page-header{
    width:100%;
    margin:0px;
    border:1px solid #e4e4e4;
    background-color:#fff;
	
}
.page-header .prompt {
    width:82%;
    margin:0 auto;
    margin-top:18px;
    background-color:#fefdf9;
    border:1px solid #ffe2aa;
    margin-bottom:34px;
}
.page-header .tableq{
    width:94%;
