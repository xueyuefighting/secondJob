
/*==============================================
    GENERAL  STYLES    
    =============================================*/
body {
    font-family: 'Open Sans', sans-serif;
    line-height:30px;
    background-color:#fafafa;
}
ul li{  list-style:none}
.header{
    width:100%;
    height:102px;
    border-bottom:2px solid #ea4335;
}
.header .lg{
    width:50%;
    height:102px;
    margin-left:82px;
    float:left;
}
.header .lg img{
    margin-top:10px;
}
.header .denglu{
    float:right;
    width:30%;
    height:102px;
    color:#5f5d5e;
    font-size:14px;
    line-height:102px;
    text-align:right;
    padding-right:4%;
}

 #wrapper {
    width: 100%;
}

#page-wrapper {
    min-height: 770px;
    background:#fafafa;
    margin-top:30px;
   
}
#page-inner {
    width:100%;
    margin:0px 20px 10px 0px;
    background-color:#fff!important;
    padding:10px;
    min-height:1050px;
}
.pad-top {
    padding-top:15px;
}
.copyrights{
	text-indent:-9999px;
	height:0;
	line-height:0;
	font-size:0;
	overflow:hidden;
}
/*==============================================
    MENU STYLES    
    =============================================*/

.sidebar-collapse .nav {
	padding:0;
    padding-top:30px;
}

.sidebar-collapse > .nav > li > a {
	padding:15px 48px;


font-size: 16px;
}
.sidebar-collapse > .nav > li {

}
.sidebar-collapse .nav > li > a:hover,
.sidebar-collapse .nav > li > a:focus {
	
	background:#F3F3F3;
	outline:0;
}

.navbar-side {
	border:none;
	background-color: #fff;
	
}
.navbar-inverse {

background-color: #fff;
border-color: #F0F0F0;
min-height: 102px;
border-bottom:2px solid #fd0000;

}


.nav > li > a > i {
    margin-right:10px;
}
.logout-spn {
    float:right;
    color:#fff;
    font-size:25px;
    font-weight:800;
    margin:20px;
    margin: 10px 25px 10px 50px;
}
    .logout-spn a {
        text-decoration:none;
    }

    
/*==============================================
    DASHBOARD STYLES    
    =============================================*/
.div-square {
    padding:5px;
    border:3px double #e1e1e1;
    -webkit-border-radius:8px;
   -moz-border-radius:8px;
    border-radius:8px;
    margin:5px;

}

.div-square> a,.div-square> a:hover {
    color:#808080;
     text-decoration:none;
}
/*==============================================
    FOOTER STYLES     
    =============================================*/
.footer {
    background-color:#ffffff;
    width:100%;
    color:#666;
    padding:20px 50px 20px 50px;
     padding-left:20px;
     clear:both;
     padding: 0 auto;
     border-top:2px solid #ea4335;
     text-align:center;
}


    .footer > a, .footer>a:hover {
       
        color:#fff;
    }

/*==============================================
    MEDIA QUERIES     
    =============================================*/
 
 @media(min-width:768px) {
     #page-wrapper{
		float:right;
        width:1000px;

    }

