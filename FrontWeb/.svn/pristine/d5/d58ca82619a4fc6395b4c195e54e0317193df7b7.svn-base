   <script src="../../js/lib/me/jquery-3.1.1.min.js"></script>
  <script src="../../js/lib/me/jquery.base64.js"></script>
      <!-- BOOTSTRAP SCRIPTS -->
    <script src="../../js/lib/bootstrap/bootstrap.min.js"></script>
  <script src="../../js/lib/common.js"></script>
    <script type="text/javascript">
    	$(document).ready(function(){
			var str = getQueryString("txRequest");
			str = $.base64.decode($.trim(str));
			if(str==""){
				alert("地址有误！");
				return;
			}
			var $json = JSON.parse(str);
			var PlatformId = $json.PlatformId;
			var InterfaceNo = $json.InterfaceNo;
			var UrlParam = $json.UrlParam;
			var Version = $json.Version;
			var SourceType = $json.SourceType;
			var JsonMessage = $json.JsonMessage;
			var $JsonMessage = JSON.parse(JsonMessage);
			var FundID = $JsonMessage.FundID;
			var UserRole = $JsonMessage.UserRole;
			var ReturnURL = $JsonMessage.ReturnURL;
			var MerchantUserID = $JsonMessage.MerchantUserID;
			if(!UserRole || UserRole==""){
			scf.confirm("用户类型有误", scf.confirm.typeEnum.error);
				window.history.back();
			}
		var url = "identity.html?";//Common.ProjectName+"/resources/u_scf/
		if(UserRole==20){
			url = "personalVerify.html?";//Common.ProjectName+"/resources/u_scf/
		}
		url+="PlatformId="+PlatformId+"&InterfaceNo="+InterfaceNo+"&ReturnURL="+ReturnURL+"&TerminalSource="+SourceType+"&FundID="+FundID+"&UserRole="+UserRole+
		     "&MerchantUserID="+MerchantUserID;
		window.open(encodeURI(url)
				,"_self","location=no");
		});
function getQueryString(name) {
	 var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
	 var r = window.location.search.substr(1).match(reg);
	 if (r != null) return unescape(r[2]);
	 return null;
}
    </script>
</head>

